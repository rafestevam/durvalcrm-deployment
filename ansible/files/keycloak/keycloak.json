{
  "realm": "durval-crm",
  "auth-server-url": "https://20.127.155.169:9443/",
  "ssl-required": "external",
  "resource": "durvalcrm-app",
  "verify-token-audience": true,
  "credentials": {
    "secret": "keycloak-client-secret-to-be-configured"
  },
  "use-resource-role-mappings": false,
  "confidential-port": 0,
  "policy-enforcer": {},
  "cors": true,
  "cors-max-age": 1000,
  "cors-allowed-headers": "X-Custom-Header",
  "cors-allowed-methods": "GET,POST,PUT,DELETE,OPTIONS",
  "cors-exposed-headers": "X-Custom-Header",
  "expose-token": true,
  "bearer-only": false,
  "enable-basic-auth": false,
  "disable-trust-manager": true,
  "allow-any-hostname": true,
  "connection-pool-size": 20,
  "socket-timeout-millis": 5000,
  "connection-timeout-millis": 5000,
  "connection-ttl-millis": 5000,
  "disable-cookies": false,
  "principal-attribute": "preferred_username",
  "proxy-url": "",
  "redirect-rewrite-rules": {},
  "token-minimum-time-to-live": 10,
  "min-time-between-jwks-requests": 10,
  "public-key-cache-ttl": 86400
}